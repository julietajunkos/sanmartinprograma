Algoritmo aumento
	Escribir "ingrese valor en pesos del producto en enero "
	leer precio1
	Escribir "ingrese el valor en pesos del producto en diciembre "
	leer precio2
	definir aum Como Real
	aum = (precio2-precio1)/precio1
	precio_final = aum*100
	escribir "el porcentaje de aumento del producto en un año es igual a " precio_final "%"
	
FinAlgoritmo
